
<h1>Add Projects</h1>
<%= form_for :project,  url: projects_path  do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field(:title,{:class => 'form-control'}) %>
  </p>

  <p>
    <%= f.label :Description %><br>
    <%= f.text_area(:body,{:class => 'form-control'}) %>
  </p>

  <p>
    <%= f.submit({:class => 'btn btn-primary'} )  %>
  </p>
  
<% end %>



  
